		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^11.2.7",
		"eslint": "^8.57.1",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-storybook": "^0.12.0",
		"jest": "^27.5.1",
		"prettier": "^3.5.3",
		"prop-types": "^15.8.1",
		"storybook": "^8.6.14",
		"webpack": "^5.99.8"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:storybook/recommended"
