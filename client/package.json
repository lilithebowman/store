		"@mui/material": "^6.1.6",
		"axios": "^1.7.7",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.30.1",
		"react-scripts": "^5.0.1",
		"styled-components": "^6.1.13"
	},
	"devDependencies": {
		"@storybook/addon-essentials": "^8.6.14",
		"@storybook/addon-interactions": "^8.6.14",
		"@storybook/addon-links": "^8.6.14",
